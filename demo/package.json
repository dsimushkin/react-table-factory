{
  "name": "react-table-factory-demo",
  "version": "0.1.0",
  "homepage": "http://dsimushkin.github.io/react-table-factory/",
  "private": true,
  "dependencies": {
    "node-sass": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-scripts": "latest",
    "react-table-factory": "^0.2.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
